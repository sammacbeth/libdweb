{
  "manifest_version": 2,
  "version": "0.1",
  "name": "tcp@libdweb",
  "description": "Extension containing libdweb TCPSocket API",
  "permissions": [],

  "experiment_apis": {
    "TCPSocket": {
      "schema": "../../src/toolkit/components/extensions/tcp.json",
      "child": {
        "scopes": ["addon_child"],
        "paths": [["TCPSocket"]],
        "script": "../../src/toolkit/components/extensions/ext-tcp.js"
      }
    }
  },
  "icons": {
    "48": "../discovery/logo-flyweb-grey.svg",
    "96": "../discovery/logo-flyweb-grey.svg"
  },
  "sidebar_action": {
    "default_icon": {
      "16": "../discovery/logo-flyweb-grey.svg",
      "32": "../discovery/logo-flyweb-grey.svg"
    },
    "default_title": "TCPSocket",
    "default_panel": "terminal.html"
  }
}
